generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  bal   Int
  email String @unique
}

model Identifier {
  email String @id @default(uuid()) @map("_id")
  id String @unique
}